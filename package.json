{
  "name": "twinte-server",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "ssh://git@github.com/twin-te/twinte-server.git",
  "author": "Sosuke Suzuki <aosukeke@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "ts-node -T src/index.ts",
    "start": "cross-env NODE_ENV='production' run-s swagger dev",
    "clean": "rm -rf ./build",
    "build": "yarn clean && tsc --skipLibCheck",
    "format": "prettier --write \"{,src/**/}*.{ts,md,js,json}\"",
    "lint": "eslint ./ --ext .ts",
    "lint:fix": "yarn lint --fix",
    "swagger": "yarn swaggerGen -c src/infrastructure/rest-api/swaggerConfig.json",
    "updateLectureDatabase": "ts-node -T src/admin-scripts/upsertLectureDatabase.ts",
    "importModuleDefinition": "ts-node -T src/admin-scripts/importModuleDefinition.ts",
    "importSubstituteDefinition": "ts-node -T src/admin-scripts/importSubstituteDefinition.ts",
    "importEventDefinition": "ts-node -T src/admin-scripts/importEventsDefinition.ts"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/cli-progress": "^1.8.1",
    "@types/connect-pg-simple": "^4.2.0",
    "@types/express": "^4.17.2",
    "@types/jest": "^24.0.24",
    "@types/moment": "^2.13.0",
    "@types/node": "^12.0.0",
    "@types/passport": "^1.0.1",
    "@types/passport-google-oauth": "^1.0.41",
    "@types/passport-twitter": "^1.0.34",
    "@typescript-eslint/parser": "^2.3.1",
    "eslint": "^6.4.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-prettier": "^3.1.1",
    "jest": "^24.9.0",
    "nodemon": "^1.19.0",
    "prettier": "^1.17.0",
    "ts-jest": "^24.2.0",
    "ts-node": "^8.1.0",
    "typescript": "^3.7.2"
  },
  "dependencies": {
    "@types/cors": "^2.8.6",
    "@types/express-session": "^1.15.16",
    "@types/passport-google-oauth20": "^2.0.3",
    "@types/uuid": "^3.4.6",
    "axios": "^0.19.0",
    "cli-progress": "^3.4.0",
    "connect-pg-simple": "^6.0.1",
    "cors": "^2.8.5",
    "cross-env": "^6.0.3",
    "express": "^4.16.4",
    "express-session": "^1.17.0",
    "inversify": "^5.0.1",
    "log4js": "^6.1.0",
    "moment": "^2.24.0",
    "npm-run-all": "^4.1.5",
    "passport": "^0.4.0",
    "passport-apple": "^1.0.0",
    "passport-google-oauth": "^2.0.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-twitter": "^1.0.4",
    "pg": "^7.12.1",
    "reflect-metadata": "^0.1.13",
    "stripe": "^8.4.0",
    "twinte-parser": "^1.2.2",
    "typeorm": "^0.2.20",
    "typescript-rest": "^2.2.2",
    "typescript-rest-swagger": "^1.0.4",
    "uuid": "^3.3.3"
  }
}
